{
    "url": "http://services.coastalresilience.org:6080/arcgis/rest/services/GSVG/GSVG_Flood_and_SLR_Data/MapServer",
    "controls": [
        {
            "type": "header",
            "text": "1. Select the geography"
        },
        {
            "type": "group",
            "control": "radio"
        },
        {
            "type": "header",
            "text": "2. Select the amount of flooding"
        },
        {
            "type": "group",
            "title": "Sea Level Rise Scenario",
            "control": "slider",
			"options": [
				{
                    "text": "None",
                    "selected": true,
                    "value": "None"
                },
				{
                    "text": "1 meter",
					"help": "placeholder",
                    "selected": false,
                    "value": "1 meter"
                },
				{
                    "text": "2 meter",
					"help": "placeholder",
                    "selected": false,
                    "value": "2 meter"
                }
			]
        },
        {
            "type": "group",
            "title": "Storm Surge Scenario",
            "control": "radio",
            "options": [
                {
                    "text": "None",
                    "selected": true,
                    "value": "None"
                },
                {
                    "text": "Hurricane Lenny",
					"help": "placeholder",
                    "selected": false,
                    "value": "Hurricane Lenny"
                },
                {
                    "text": "Hurricane Ivan",
					"help": "placeholder",
                    "selected": false,
                    "value": "Hurricane Ivan"
                },
                {
                    "text": "100 year Flood",
					"help": "placeholder",	
                    "selected": false,
                    "value": "100 year Flood"
                }
			]
        }
    ]
}


